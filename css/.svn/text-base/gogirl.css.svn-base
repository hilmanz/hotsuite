/*
Theme Name: GoGirl
Author: Acit Jazz
Version: 1.0
*/
* {
	outline:none !important;
	font-family: "HelveticaNeue-Light", "Helvetica Neue Light", "Helvetica Neue", Helvetica, Arial, "Lucida Grande", sans-serif;
	font-weight: 300;
}
::selection {
 background: #e42425;
 color: #FFF;
}
::-moz-selection {
color: #fff;
background-color: #e42425;
}
a {
	color: #e85597;
	text-decoration:none !important;
	-webkit-transition: all 0.3s ease-in-out;
	-moz-transition: all 0.3s ease-in-out;
	-o-transition: all 0.3s ease-in-out;
	-ms-transition: all 0.3s ease-in-out;
	transition: all 0.3s ease-in-out;
}
a:hover {
	color: #e85597;
}
a:focus {
	color: #e85597;
}
body {
	
	background:url(data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAAAoAAAAICAYAAADA+m62AAAAPUlEQVQYV2NkQAP3Xr75ryQuwogujiIAU4RNMVwhuiQ6H6wQl3XI4oy4FMHcCJPHcDS6J2A2EqUQpJhohQDr8ye7Cjfh7gAAAABJRU5ErkJggg==) repeat #ecf8fb;
}
#preloader {
	position: fixed;
	top:0;
	left:0;
	right:0;
	bottom:0;
	background-color:#fff; /* change if the mask should have another color then white */
	z-index:99; /* makes sure it stays on top */
}
#status {
	width:200px;
	height:200px;
	position:absolute;
	left:50%; /* centers the loading animation horizontally one the screen */
	top:50%; /* centers the loading animation vertically one the screen */
	background-image:url(../images/status.gif); /* path to your loading animation */
	background-repeat:no-repeat;
	background-position:center;
	margin:-100px 0 0 -100px; /* is width and height divided by two */
}
/* ================= LOGO  ==================== */
.logo {
	padding:20px 0;
}
.logo a {
	padding:0 10px;
}
/* ================= NAVIGATION  ==================== */
.navigation {
	background:#fff;
	border-bottom:solid 1px #69d9df;
	position:relative;
}
#menu{
	margin:0;
}
.navbar {
	float:right;
	padding:30px 35px 0 0;
	margin:0;
	border:none;
	border-radius:0;
}
.navbar a {
	display:inline-block;
	padding:7px 14px;
	text-transform:uppercase;
	color:#000;
	height:63px;
	position:relative;
}
.navbar a>span {
	position:relative;
	z-index:10;
}
.navbar li {
	float:left;
	width:auto;
	overflow:hidden;
	margin:0 1px 0 0;
}
.navbar li>a:before {
	background:#69d9df;
	content:'';
	position:absolute;
	width:100%;
	display:block;
	height:63px;
	left:0;
	bottom:-63px;
	-webkit-transition: all 0.3s ease-in-out;
	-moz-transition: all 0.3s ease-in-out;
	-o-transition: all 0.3s ease-in-out;
	-ms-transition: all 0.3s ease-in-out;
	transition: all 0.3s ease-in-out;
}
.navbar li.current>a:before, .navbar li:hover>a:before {
	left:0;
	bottom:0;
}
.navbar a:hover, .navbar li.current a {
	color:#fff;
}
.slicknav_nav{
	padding:20px 0;
}
.slicknav_menu{
	background:none;
	padding:0;
}
.slicknav_btn{
	position:absolute;
	z-index:100;
	right:58px; top:24px;
	margin:0;
	width:40px; height:40px;
}
.slicknav_menu .slicknav_no-text{
	position:relative;
}
.slicknav_menu .slicknav_icon{
	float:none;
}
.slicknav_menu .slicknav_icon-bar{
	width:100%; height:3px;
	margin:4px 0;
}
.slicknav_nav a{
	color:#333;
	padding:10px 25px;
	margin:0;
	border-bottom:solid 1px #eee;	
}
.slicknav_menu:before, .slicknav_menu:after{
	display:none;
}
/* ================= SLIDER  ==================== */
#slider {
	padding:30px 0;
}
/* ================= SEARCH FORM  ==================== */
#header .container{
	position:relative;
}
.searchbox-container{
	display:none;
}
.search-form-overlay {
	position:fixed;
	width:100%; height:100%;
	background:rgba(255,255,255,0.9);
	left:0; top:0;
	z-index:100;
}
.searchbox{
	position:absolute;
	bottom:0;
	width:100%;
}
.search-form{
	z-index:110;
	position:relative;
}
.formbox{
	width:100%;
}
.search-form input[type=text] {
	border: none;
	border-bottom:solid 1px #21358a;
	padding: 10px;
	width:100%;
	background:none;
	font-size:18px;
}
.search-form label{
	font-weight:100;
	font-size:12px;
	position:absolute;
	bottom:-25px;
	color:#21358a;
	text-transform:none;
	width:100%;
	left:0;
}
.btn-search {
	border: none;
	background: none;
	font-size: 26px;
	float: left;
	width: 30px;
	padding: 0 5px;
	position:absolute;
	right:20px;
	top:28px;
}
.search-form .btn-search{
	top:-20px;
	right:20px;
	color:#21358a;
}
.close-search{
	background:url(../images/x.png) no-repeat;
	width:65px; height:65px;
	position:absolute;
	top:20px;
	right:0;
	display:block;
	z-index:110;
}
/* ================= THUMBNAIL  ==================== */
.thumb {
   overflow: hidden;
   position: relative;
   text-align: center;
   display:block;
}
.thumb img {
	width:100%;
	height:auto;
	margin: 0;
	height: auto;
   display: block;
   position: relative;
   -webkit-transition: all 0.2s linear;
   -moz-transition: all 0.2s linear;
   -o-transition: all 0.2s linear;
   -ms-transition: all 0.2s linear;
   transition: all 0.2s linear;
}
.thumb:hover img {
   -webkit-transform: scale(1.1,1.1);
   -moz-transform: scale(1.1,1.1);
   -o-transform: scale(1.1,1.1);
   -ms-transform: scale(1.1,1.1);
   transform: scale(1.1,1.1);
}
.thumbs img{
	width:100%;
}
.thumb-small img {
	width:50px;
	height:50px;
	display:inline-block;
}
.thumb-small {
	float:left;
	padding:0 10px 0 0;
}
.item .thumb-small img {
	width:40px;
	height:40px;
}
/* ================= BANNER  ==================== */
.banner-ads img {
	width:100%;
	height:auto;
}
.banner-ads{
	position:relative;
}
.banner-ads .button{
	position:absolute;
	bottom:20px;
	right:30px;
}
/* ================= CONTENT  ==================== */
#sidebar {
	padding:0;
}
#content {
	padding-right:0;
}
#main-content {
	margin:0 0 50px 0;
}
#main-content>div {
	position:relative;
}
.fr{ float:right; }
.fl{ float:left; }
/* ================= WIDGET  ==================== */
.widget {
	margin:0 0 30px 0;
	overflow:hidden;
}
.widget-title h3 {
	margin:0;
	padding:0 20px;
	font-style:italic;
	text-transform:uppercase;
	display:inline-block;
	background:#000;
	color:#fff;
	line-height:30px;
	font-size:16px;
	font-weight:100;
	letter-spacing:2px;
	position:relative;
}
.widget-title h3.has-arrow {
	padding:0 70px 0 20px;
	position:relative;
}
.widget-title h3.has-arrow:after {
	font-family: 'eufont';
	font-weight: 100;
	font-style: normal;
	content: "\ea6d";
	position:absolute;
	right:10px;
	font-size:20px;
}
.widget-list ul {
	list-style:none;
	margin:0;
	padding:0;
}
.widget-list li {
	list-style:none;
	margin:0;
	padding:10px 0;
	overflow:hidden;
	position:relative;
}
.widget-list li:after {
	content:'';
	position:absolute;
	width:70%;
	bottom:0;
	right:0;
	height:1px;
	background:#3dc2c9;
}
.featured-girls .widget-content {
	background:#69d9df;
	padding:0;
	position:relative;
}
.featured-girls .widget-content:before{
	content:"";
	background:url(data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAAAoAAAAICAYAAADA+m62AAAAPUlEQVQYV2NkQAP3Xr75ryQuwogujiIAU4RNMVwhuiQ6H6wQl3XI4oy4FMHcCJPHcDS6J2A2EqUQpJhohQDr8ye7Cjfh7gAAAABJRU5ErkJggg==) repeat #ecf8fb;
	position:absolute;
	bottom:0;
	right:0;
	z-index:10;
	border-width:0 0 30px 30px;
	border-style:solid;
	border-color:transparent #3dc2c9;
	/* css3 extras */
	-webkit-box-shadow:0 1px 1px rgba(0,0,0,0.3), 
				  -1px 1px 1px rgba(0,0,0,0.2);
	-moz-box-shadow:0 1px 1px rgba(0,0,0,0.3), 
				  -1px 1px 1px rgba(0,0,0,0.2);
		box-shadow:0 1px 1px rgba(0,0,0,0.3), 
				  -1px 1px 1px rgba(0,0,0,0.2);
}
.featured-girls .thumb-small {
	width:30%;
	text-align:center;
	float:left;
}
.featured-girls h6 {
	float:left;
	margin:0;
	width:70%;
	padding:0 10px;
	line-height:50px;
	font-size:14px;
	color:#000;
}
.latest-story .thumb:after {
	content:'';
	position:absolute;
	bottom:0;
	width:100%;
	height:100%;
	background: rgba(255, 255, 255, 0);
	background: -moz-linear-gradient(top, rgba(255, 255, 255, 0) 30%, rgba(255, 255, 255, 0) 46%, rgba(255, 255, 255, 0.04) 49%, rgba(0, 0, 0, 0.75) 100%);
	background: -webkit-gradient(left top, left bottom, color-stop(30%, rgba(255, 255, 255, 0)), color-stop(46%, rgba(255, 255, 255, 0)), color-stop(49%, rgba(255, 255, 255, 0.04)), color-stop(100%, rgba(0, 0, 0, 0.75)));
	background: -webkit-linear-gradient(top, rgba(255, 255, 255, 0) 30%, rgba(255, 255, 255, 0) 46%, rgba(255, 255, 255, 0.04) 49%, rgba(0, 0, 0, 0.75) 100%);
	background: -o-linear-gradient(top, rgba(255, 255, 255, 0) 30%, rgba(255, 255, 255, 0) 46%, rgba(255, 255, 255, 0.04) 49%, rgba(0, 0, 0, 0.75) 100%);
	background: -ms-linear-gradient(top, rgba(255, 255, 255, 0) 30%, rgba(255, 255, 255, 0) 46%, rgba(255, 255, 255, 0.04) 49%, rgba(0, 0, 0, 0.75) 100%);
	background: linear-gradient(to bottom, rgba(255, 255, 255, 0) 30%, rgba(255, 255, 255, 0) 46%, rgba(255, 255, 255, 0.04) 49%, rgba(0, 0, 0, 0.75) 100%);
	filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#ffffff', endColorstr='#000000', GradientType=0 );
	left:0;
}
.latest-story .col-md-6 {
	padding:0;
	position:relative;
	border-bottom:solid 4px #e85597;
}
.story-box {
	position:relative;
	display:block;
	overflow:hidden;
	color:#fff;
	-webkit-transition: none;
	-moz-transition: none;
	-o-transition: none;
	-ms-transition: none;
	transition: none;
}
.story-box .caption {
	position:absolute;
	bottom:-30px;
	width:100%;
	left:0;
	padding:0 20px 10px 20px;
	z-index:10;
	-webkit-transition: all 0.3s ease-in-out;
	-moz-transition: all 0.3s ease-in-out;
	-o-transition: all 0.3s ease-in-out;
	-ms-transition: all 0.3s ease-in-out;
	transition: all 0.3s ease-in-out;
}
.story-box .caption:before {
	content:'';
	width:100%;
	position:absolute;
	height:0px;
	background:#e85597;
	bottom:0;
	left:0;
	-webkit-transition: all 0.3s ease-in-out;
	-moz-transition: all 0.3s ease-in-out;
	-o-transition: all 0.3s ease-in-out;
	-ms-transition: all 0.3s ease-in-out;
	transition: all 0.3s ease-in-out;
}
.story-box:hover .caption:before {
	height:80px;
}
.story-box:hover .caption {
	bottom:0;
}
.story-box:hover {
	color:#000;
}
.caption h4 {
	overflow: hidden;
	text-overflow: ellipsis;
	white-space: nowrap;
	position:relative;
	z-index:10;
	font-weight:700;
	margin:0;
}
.story-box .meta-post {
	position:relative;
	z-index:10;
	-webkit-transition: all 0.3s ease-in-out;
	-moz-transition: all 0.3s ease-in-out;
	-o-transition: all 0.3s ease-in-out;
	-ms-transition: all 0.3s ease-in-out;
	transition: all 0.3s ease-in-out;
	bottom:-40px;
	color:#000;
}
.meta-post {
	font-size:12px;
	margin:0;
}
.social-post {
	margin:0 0 20px 0;
}
.social-post a {
	color:#000;
	font-size:12px;
	margin:0 10px 0 0;
}
.social-post a:hover{
	color:#e85597;
}
.social-post a.fb-share:hover{
	color:#196cc1;
}
.social-post a.twitter-share:hover{
	color:#3ebfda;
}
.social-post a>i {
	font-size:14px;
	position:relative;
	top:2px;
}
.story-box:hover .meta-post {
	bottom:0;
}
.post-date {
	display:block;
}
.popular-story .widget-content {
	padding:40px 30px;
	background:#fff;
	overflow:hidden;
}
.entry h4 {
	font-weight:700;
}
.entry h4 a {
	color:#000;
	font-weight:700;
	font-size:26px;
}
.category-banner .col-md-4 {
	padding:0;
	position:relative;
}
.caption-title {
	position:absolute;
	color:#000;
	top:35%;
	text-transform:uppercase;
	font-style:italic;
	text-align:center;
	width:100%;
}
.caption-title h4 {
	display:inline-block;
	font-style:normal;
	padding:0 15px;
	font-size:18px;
	line-height:30px;
	position:relative;
	letter-spacing:2px;
	margin:0;
	z-index:10;
	font-family:Calibri, Arial;
	color:#fff;
}
.category-banner-box {
	display:block;
	position:relative;
	background:#000;
}
.category-banner-box .caption-title{
	top:5px;
	right:0;
	width:auto;
}
.category-banner-box:before {
	display:none
	content:'';
	position:absolute;
	width:1%;
	height:1%;
	background:#e85597;
	opacity:0;
	left:50%;
	top:50%;
	-webkit-transition: all 0.3s ease-in-out;
	-moz-transition: all 0.3s ease-in-out;
	-o-transition: all 0.3s ease-in-out;
	-ms-transition: all 0.3s ease-in-out;
	transition: all 0.3s ease-in-out;
}
.category-banner-box:hover:before{
	width:100%; height:100%;
	left:0; top:0;
	opacity:0.7;
}
.category-banner-box:hover .caption-title h4{
	background:none;
}
.aboutlink, .copy {
	position:fixed;
	left:50%;
	color:#000;
	bottom: 120px;
	width:160px; display:block;
	margin:0 0 0 -590px;
}
.aboutlink {
	transform: rotate(-90deg);
	text-transform:uppercase;
}
.copy {
	transform: rotate(90deg);
	font-size:12px;
	text-align:right;
	margin:0 0 0 430px;
}
/* ================= TAB  ==================== */
.tabs {
	border-radius: 4px;
	margin-bottom: 35px;
}
.nav-tabs {
	margin: 0 !important;
	font-size: 0;
	border-bottom-color: #EEE;
}
.nav-tabs li {
	display: inline-block;
	float: none;
	margin:0 !important;
}
.nav-tabs li:last-child a {
	margin-right: 0;
}
.nav-tabs li a {
	border-radius: 5px 5px 0 0;
	font-size: 14px;
	margin-right: 1px;
}
.nav-tabs li a, .nav-tabs li a:hover {
	background: #F4F4F4;
	border-bottom: none;
	border-left: 1px solid #EEE;
	border-right: 1px solid #EEE;
	border-top: 3px solid #EEE;
	color: #333;
}
.nav-tabs li a:hover {
	border-bottom-color: transparent;
	border-top: 3px solid #f15a24;
	box-shadow: none;
}
.nav-tabs li a:active, .nav-tabs li a:focus {
	border-bottom: 0;
}
.nav-tabs li.active a, .nav-tabs li.active a:hover, .nav-tabs li.active a:focus {
	background: #FFF;
	border-left-color: #EEE;
	border-right-color: #EEE;
	border-top: 3px solid #f15a24;
	color: #333;
}
.tab-content {
	border-radius: 0 0 4px 4px;
	box-shadow: 0 1px 5px 0 rgba(0, 0, 0, 0.04);
	background-color: #FFF;
	border: 1px solid #EEE;
	border-top: 0;
	padding: 15px;
}
/* ================= CAROUSEL  ==================== */
.carousel {
	margin:0 0 50px 0;
}
.carousel-control {
	background:none;
	background-image:none !important;
}
.carousel-control .glyphicon {
	background:rgba(0, 0, 0, .6);
	border-radius:100px;
}
.glyphicon-chevron-left:before {
	content: "\ea6c";
}
.glyphicon-chevron-right:before {
	content: "\ea6d";
}
.carousel-inner>.item>img, .carousel-inner>.item>a>img {
	display:inline-block;
}
.carousel-indicators {
	left:0;
	width:100%;
	text-align:center;
	bottom:-30px;
	margin:0 !important;
}
.carousel-indicators li {
	border:1px solid #333;
	display:inline-block;
	margin:0 3px !important;
}
.carousel-indicators .active {
	width:10px;
	height:10px;
	background:#333;
}
.carousel-indicators li:hover {
	background:#333;
}
/* ================= COLLAPSE ACCORDION  ==================== */
.panel-title>a {
	display:block;
}
/* ================= PAGE  ==================== */
.page-title {
	color:#213488;
	padding:30px 0 0 0;
	overflow:hidden;
}
.page-title h2 {
	font-weight:900;
	font-size:44px;
	text-align:right;
	position:relative;
	display:inline-block;
	padding:10px 0;
	float:right;
	text-transform:uppercase;
}
.page-title .col-md-12{
	width:80%;
	float:none;
	margin:0 auto;
}
.searchpage .page-title h2{
	font-size:24px
}
.page-title h2:after {
	content:'';
	position:absolute;
	height:2px;
	bottom:0;
	width:80%;
	max-width:200px;
	right:0;
	background:#213488;
}
.page-desc {
	width:80%;
	padding:34px 0 0 0;
}
.entry-detail{
	padding:30px 20px;
	background:#fff;
	margin:60px 0 0 0;
	overflow:hidden;
}
.pagecontainer{
	padding:30px 20px;
	background:#fff;
	margin:60px 0 0 0;
	position:relative;
	overflow:hidden;
}
.thankspage .pagecontainer{
	min-height:400px;
	padding:100px 30px;
}
.thankspage .page-desc{
	width:100%;
}
.entry-detail h1{
	font-weight:900;
	margin:0 0 20px 0;
}
.entry-container{
	padding:30px 0;
}
.entry-container img{
	max-width:100%;
}
.theContent{
	padding:40px 0 0 0;
	overflow:hidden;
}
/* ================= COMMENT  ==================== */
#comment{
	margin:50px 0 0 0;
}
#commentform textarea,#commentform input[type=text],#commentform input[type=email]{
	border:solid 1px #ddd;
	padding:7px;
	margin:0 0 20px 0;
	display:block;
	float:none;
	width:100%;
}
#commentform{
}
.comment-title{
	margin:0 0 30px 0;
}
.comment-title h3{
	font-weight:700;
	margin:0 0 10px 0;
}
#respond{
	clear:both;
}
#respond ul{
	margin:0; padding:0;
}
#respond li{
	border-bottom:solid 1px #ddd;
	padding:10px 0;
	list-style:none;
}
.reply-content{;
}
.comment-author{
	margin:0 0 10px 0;
}
.comment-author h5{
	margin:0;
	font-weight:700;
}
/* ================= BUTTON  ==================== */
.button{
	display:inline-block;
	border:none;
	padding:0 30px;
	line-height:35px;
	background:#e85597;
	color:#fff;
	text-decoration:none;
	text-transform:uppercase;
	min-width:180px;
	text-align:center;
}
.button:hover{
	background:#69d9df;
	color:#fff;
}
.row-btn{
	clear:both;
}
.paging{
	text-align:center;
	padding:30px 0;
}
.paging a{
	display:inline-block;
	vertical-align:top;
	line-height:40px;
	width:40px;
	text-align:center;
	background:#fff;
	color:#000;
	font-size:16px;
}
.paging a.current{
	background:#213488;
	color:#fff;
}
/* ================= FORM  ==================== */
.theform .row{
	margin:0 0 30px 0;
}
.theform{
	padding:30px 10px;
}
.theform i{
	font-size:12px;
	font-style:italic;
	color:#666;
	display:block;
}
label{
	font-weight:300;
	text-transform:uppercase;
}
.theform p{
	font-weight:300;
	font-size:12px;
	margin:0 0 20px 0;
}
input[type=text],textarea{
	width:100%;
	padding:7px;
	resize:none;
	border:solid 1px #ddd;
}
.checkbox-row{
	position:relative;
	margin:0 0 10px 0;
}
input[type=radio],input[type=checkbox]{
	opacity:0;
	position:absolute;
	left:0; top:0;
	z-index:1000;
	cursor:pointer;
	width:30px; height:30px;
}
input[type=radio] + span,input[type=checkbox] + span{
	display: inline-block;
	line-height:30px;
	vertical-align:top;
}
input[type=radio]:checked + span:before,input[type=checkbox]:checked + span:before {
	font-family: 'eufont';
	font-weight: 100;
	font-style: normal;
	content: "\ea6b";
	display: inline-block;
	font-size:20px;
	color:#69d9df;
}

input[type=radio] + span:before,input[type=checkbox] + span:before {
	content: "";
	display: inline-block;
	font-size:20px;
	width:30px; height:30px;
	border:solid 1px #ddd;
	margin:0 10px 0 0;
	vertical-align:top;
	line-height:30px;
	text-align:center;
}
label.error{
	color:#c00;
	font-size:12px;
	display:none !important;
}
input.error,textarea.error{
	border:solid 1px #e85597;
}
.errorbox{
	background:#e85597;
	color:#fff;
	text-align:center;
	text-transform:uppercase;
	position:absolute;
	top:0; left:0;
	width:100%;
	display:none;
}
.errorbox p{
	margin:0;
	line-height:40px;
}
div.file_upload {
	width: 100px;
	height: 30px;
	display: inline;
	position: absolute;
	overflow: hidden;
	cursor: pointer;
	color: #69d9df;
	text-align: center;
	padding-top: 8px;
	right:0;
	top:0;
}

div.file_upload:before {
	content: 'CHOOSE FILE';
	position: absolute;
	left: 0; right: 0;
	text-align: center;	
	cursor: pointer;
	font-size:12px;
}

div.file_upload input {
	position: relative;
	height: 30px;
	width: 250px;
	display: inline;
	cursor: pointer;
	opacity: 0;
}
.inputfile{
	position:relative;
}

/* ================= ISOTOPE  ==================== */
#isotope {
	clear:both;
}
.postlist {
	margin-left:-10px;
	margin-right:-10px;
}
.postlist .item {
	padding:10px;
	height:auto;
}
.item h4,.item h4 a {
	font-weight:700;
	font-size:20px;
	color:#000;
}
.item .thumb img {
	border:solid 1px #ececec;
}
figcaption {
	background:#fff;
	padding:20px;
	height:auto;
}
.postlist .isotope-item {
	-webkit-transition-duration: 0.8s;
	-moz-transition-duration: 0.8s;
	transition-duration: 0.8s;
	-webkit-transition-property: -webkit-transform, opacity;
	-moz-transition-property: -moz-transform, opacity;
	transition-property: transform, opacity;
}
.readmore {
	color:#000;
}
/* ================= LOADER  ==================== */
.loaderbox{
    position:relative;
	text-align:center;
}

.preloader{
	position:relative;
	display:inline-block;
	margin:0 0 0 -50px;
}
.preloader:before{
	width:20px;
	height:20px;
	border-radius:20px;
	content:'';
	position:absolute;
	background:#69d9df;
	-webkit-animation: preloader_before 1.5s infinite ease-in-out;
	-moz-animation: preloader_before 1.5s infinite ease-in-out;
	-ms-animation: preloader_before 1.5s infinite ease-in-out;
	animation: preloader_before 1.5s infinite ease-in-out;
}

.preloader:after{
	width:20px;
	height:20px;
	border-radius:20px;
	content:'';
	position:absolute;
	background:#e85597;
	left:30px;
	-webkit-animation: preloader_after 1.5s infinite ease-in-out;
	-moz-animation: preloader_after 1.5s infinite ease-in-out;
	-ms-animation: preloader_after 1.5s infinite ease-in-out;
	animation: preloader_after 1.5s infinite ease-in-out;
}

@-webkit-keyframes preloader_before {
    0% {-webkit-transform: translateX(0px) rotate(0deg)}
    50% {-webkit-transform: translateX(30px) scale(1) rotate(260deg); background:#213488;}
  	100% {-webkit-transform: translateX(0px) rotate(0deg)}
}
@-webkit-keyframes preloader_after {
    0% {-webkit-transform: translateX(0px)}
    50% {-webkit-transform: translateX(-30px) scale(1) rotate(-260deg);background:#a61858;}
  	100% {-webkit-transform: translateX(0px)}
}

@-moz-keyframes preloader_before {
    0% {-moz-transform: translateX(0px) rotate(0deg)}
    50% {-moz-transform: translateX(30px) scale(1) rotate(260deg); background:#213488;}
  	100% {-moz-transform: translateX(0px) rotate(0deg)}
}
@-moz-keyframes preloader_after {
    0% {-moz-transform: translateX(0px)}
    50% {-moz-transform: translateX(-30px) scale(1) rotate(-260deg);background:#a61858;}
  	100% {-moz-transform: translateX(0px)}
}


@-ms-keyframes preloader_before {
    0% {-ms-transform: translateX(0px) rotate(0deg)}
    50% {-ms-transform: translateX(30px) scale(1) rotate(260deg); background:#213488;}
  	100% {-ms-transform: translateX(0px) rotate(0deg)}
}
@-ms-keyframes preloader_after {
    0% {-ms-transform: translateX(0px)}
    50% {-ms-transform: translateX(-30px) scale(1) rotate(-260deg);background:#a61858;}
  	100% {-ms-transform: translateX(0px)}
}

@keyframes preloader_before {
    0% {transform: translateX(0px) rotate(0deg)}
    50% {transform: translateX(30px) scale(1) rotate(260deg); background:#213488;}
  	100% {transform: translateX(0px) rotate(0deg)}
}
@keyframes preloader_after {
    0% {transform: translateX(0px)}
    50% {transform: translateX(-30px) scale(1) rotate(-260deg);background:#a61858;}
  	100% {transform: translateX(0px)}
}